{
  "name": "awesome-mac",
  "version": "1.11.0",
  "description": "ï£¿ This repo is a collection of awesome Mac applications and tools for developers and designers. ",
  "homepage": "https://jaywcjlove.github.io/awesome-mac",
  "funding": "https://jaywcjlove.github.io/#/sponsor",
  "main": "dist/awesome-mac.json",
  "scripts": {
    "dev": "npm run build:data && next dev",
    "start": "npm run build && npm run create:ast",
    "doc": "idoc --watch",
    "build": "idoc",
    "build:data": "node build/ast.mjs",
    "build:web": "npm run build:data && next build",
    "build:prod": "npm run build:data && npm run build && STANDALONE_OUTPUT=true next build",
    "build:docker": "npm run build:data && STANDALONE_OUTPUT=true next build",
    "build:cloudflare": "npm run build:data && next build && npx opennextjs-cloudflare build --skipNextBuild",
    "start:web": "next start",
    "start:prod": "NODE_ENV=production next start",
    "create:ast": "node build/ast.mjs",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run lint && npm run test:run",
    "prebuild": "npm run verify",
    "postbuild": "node scripts/post-build.js",
    "db:postgres:start": "docker-compose -f docker-compose.postgres.yml up -d",
    "db:postgres:stop": "docker-compose -f docker-compose.postgres.yml down",
    "db:postgres:status": "docker-compose -f docker-compose.postgres.yml ps",
    "db:migrate": "tsx scripts/run-migrations.ts migrate",
    "db:migrate:status": "tsx scripts/run-migrations.ts status",
    "db:migrate:reset": "tsx scripts/run-migrations.ts reset",
    "db:migrate:rollback": "tsx scripts/run-migrations.ts rollback",
    "db:setup": "tsx scripts/run-migrations.ts setup",
    "db:health": "tsx scripts/db-health-check.ts",
    "db:export": "tsx scripts/export-sqlite.ts",
    "db:migrate:sqlite-to-pg": "tsx scripts/migrate-to-postgres.ts",
    "db:migrate:dryrun": "tsx scripts/migrate-to-postgres.ts --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/awesome-mac.git"
  },
  "keywords": [
    "awesome",
    "awesome-mac",
    "mac",
    "macosx",
    "osx"
  ],
  "files": [
    "dist/awesome-mac.json",
    "dist/awesome-mac.zh.json"
  ],
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/jaywcjlove/awesome-mac/issues"
  },
  "dependencies": {
    "@faceless-ui/modal": "3.0.0",
    "@faceless-ui/scroll-info": "2.0.0",
    "@payloadcms/db-sqlite": "3.69.0",
    "@payloadcms/next": "3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@sentry/nextjs": "10.32.1",
    "clsx": "2.1.1",
    "critters": "0.0.23",
    "graphql": "^16.8.1",
    "next": "15.4.10",
    "next-intl": "3.26.5",
    "payload": "3.69.0",
    "pino": "10.1.0",
    "pino-pretty": "13.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.0",
    "@eslint/eslintrc": "3.3.3",
    "@opennextjs/cloudflare": "1.14.7",
    "@playwright/test": "1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "autoprefixer": "^10.4.21",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "fs-extra": "^10.1.0",
    "happy-dom": "20.0.11",
    "idoc": "^1.26.5",
    "postcss": "^8.5.6",
    "remark": "14.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "11.0.0",
    "tailwindcss": "^3.4.17",
    "to-vfile": "^7.2.3",
    "typescript": "5.9.3",
    "unist-util-visit": "5.0.0",
    "vitest": "4.0.16",
    "wrangler": "4.54.0"
  }
}
